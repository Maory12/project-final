{
  "name": "projnotes-2023-class",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": " NODE_ENV=production PORT=3030 APP_URL='http://localhost' DEBUG=projnotes node ./dist/bin/www",
    "dev": "NODE_ENV=development PORT=3030 APP_URL='http://localhost' DEBUG=projnotes babel-watch --watch ./server/views ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.config.js --mode development",
    "build": "npm run clean && babel ./server --out-dir dist --copy-files",
    "builds": "npm run clean && babel ./server --out-dir dist --copy-files",
    "buildc": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "clean": "rm -rf ./dist",
    "cleanc": "rm -rf ./public/bundle.js ./public/styles/app.css"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "supports-color": "^9.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-minify": "^0.5.2",
    "babel-watch": "^7.7.2",
    "core.js": "^0.4.2",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.5",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.2",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.3"
  }
}
